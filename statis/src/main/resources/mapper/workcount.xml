<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.statis.statis.mapper.db.WorkCountMapper">

<select id="getWorkCountListPage"  parameterType="java.lang.String" resultType="workCount">
     SELECT id,publishedName,pageViewCount,publishedCount,onlineTime 
     FROM work_account w
     <where>
     <if test="name != null and name != ''">
      and  w.publishedName = #{name}
     </if>
     <if test="beginTime != null and beginTime != '' and endTime != null and endTime != ''">
     and w.onlineTime between #{beginTime} and #{endTime}
     </if>
     </where>
   </select>
   
   
</mapper>